{
  "name": "ember-apollo-client",
  "version": "2.1.0",
  "description": "An ember-cli addon for the Apollo GraphQL Client.",
  "keywords": [
    "apollo",
    "apollo-client",
    "ember",
    "ember-addon",
    "graphql",
    "graphql-client"
  ],
  "repository": "https://github.com/bgentry/ember-apollo-client",
  "license": "MIT",
  "author": "Blake Gentry",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint .",
    "start": "ember serve",
    "test": "npm-run-all lint:* test:*",
    "test:ember": "ember test"
  },
  "dependencies": {
    "apollo-cache-inmemory": "^1.6.5",
    "apollo-client": "^2.6.8",
    "apollo-link-context": "^1.0.18",
    "apollo-link-http": "^1.5.15",
    "broccoli-graphql-filter": "^0.4.1",
    "ember-auto-import": "^1.5.3",
    "ember-cli-babel": "^7.18.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@ember-decorators/object": "^6.1.0",
    "@ember/optional-features": "^1.3.0",
    "@glimmer/component": "^1.0.0",
    "@glimmer/tracking": "^1.0.0",
    "babel-eslint": "^10.1.0",
    "broccoli-asset-rev": "^3.0.0",
    "common-tags": "^1.8.0",
    "ember-cli": "~3.17.0",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-htmlbars": "^4.3.0",
    "ember-cli-inject-live-reload": "^2.0.2",
    "ember-cli-uglify": "^3.0.0",
    "ember-compatibility-helpers": "^1.2.0",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-fetch": "^8.0.1",
    "ember-load-initializers": "^2.1.1",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-qunit": "^4.6.0",
    "ember-resolver": "^7.0.0",
    "ember-source": "~3.17.0",
    "ember-source-channel-url": "^2.0.1",
    "ember-template-lint": "^2.5.0",
    "ember-try": "^1.4.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-ember": "^8.1.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "3.1.2",
    "graphql": "^14.5.8",
    "graphql-tag": "^2.10.3",
    "graphql-tools": "^4.0.7",
    "loader.js": "^4.7.0",
    "npm-run-all": "^4.1.5",
    "pretender": "^3.4.0",
    "prettier": "^2.0.4",
    "qunit-dom": "^1.1.0"
  },
  "peerDependencies": {
    "graphql": "^14.0.2"
  },
  "engines": {
    "node": "10.* || >= 12"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "before": [
      "ember-cli-babel"
    ]
  }
}
